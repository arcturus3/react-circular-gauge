    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@fontsource-variable/inter": "^5.0.8",
    "@fontsource/merriweather": "^5.0.8",
    "@fontsource/mononoki": "^5.0.7",
    "@types/chance": "^1.1.3",
    "@types/chroma-js": "^2.4.0",
    "@types/node": "^20.8.7",
