    "build:lib": "tsc && vite build --config vite.config.lib.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --config vite.config.app.ts",
    "prettier": "prettier --write ."
