    "react",
    "gauge",
    "progress-bar",
    "progress-circle",
    "component",
    "animated",
    "svg"
  ],
  "repository": "github:arcturus3/react-circular-gauge",
  "homepage": "https://arcturus3.github.io/react-circular-gauge",
