  "license": "MIT",
  "author": "Arti Schmidt",
  "type": "module",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "files": [
    "lib",
    "banner.png"
  ],
  "scripts": {
    "dev": "vite --config vite.config.app.ts",
